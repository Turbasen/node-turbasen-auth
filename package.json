{
  "name": "turbasen-auth",
  "version": "1.1.1",
  "description": "Authenticate group users in Nasjonal Turbase",
  "main": "lib/index.js",
  "directories": {
    "src": "src",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "coffee --bare --join lib/index.js --compile src/*.coffee",
    "prepublish": "coffee --bare --join lib/index.js --compile src/*.coffee",
    "postpublish": "rm -rf lib",
    "test": "INTEGRATION_TEST=true node_modules/.bin/mocha test/suite.coffee -R spec --compilers coffee:coffee-script/register",
    "watch": "INTEGRATION_TEST=true node_modules/.bin/mocha -b -c --check-leaks test/suite.coffee -R progress --compilers coffee:coffee-script/register -w"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Turistforeningen/node-turbasen-auth.git"
  },
  "keywords": [
    "SSO",
    "auth",
    "authentication",
    "nasjonal",
    "turbase",
    "turbasen",
    "turistforening",
    "turistforeningen",
    "dnt"
  ],
  "author": "Hans Kristian Flaatten <hans.kristian.flaatten@turistforeningen.no>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turistforeningen/node-turbasen-auth/issues"
  },
  "homepage": "https://github.com/Turistforeningen/node-turbasen-auth",
  "dependencies": {
    "async": "~1.2",
    "request": "~2.58"
  },
  "devDependencies": {
    "coffee-script": "~1",
    "mocha": "~2"
  },
  "engines": {
    "node": ">=0.10"
  }
}
