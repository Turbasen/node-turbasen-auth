{
  "name": "turbasen-auth",
  "version": "1.2.3",
  "description": "Authenticate group users in Nasjonal Turbase",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "hint": "node_modules/.bin/jshint index.js test.js lib/ examples/",
    "test": "INTEGRATION_TEST=true node_modules/.bin/mocha test.js -R spec",
    "watch": "INTEGRATION_TEST=true node_modules/.bin/mocha -b -c --check-leaks test.js -R progress -w"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Turistforeningen/node-turbasen-auth.git"
  },
  "keywords": [
    "SSO",
    "auth",
    "authentication",
    "dnt",
    "nasjonal turbase",
    "turbasen",
    "turistforening",
    "turistforeningen"
  ],
  "author": "Hans Kristian Flaatten <hans.kristian.flaatten@turistforeningen.no>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turistforeningen/node-turbasen-auth/issues"
  },
  "homepage": "https://github.com/Turistforeningen/node-turbasen-auth",
  "dependencies": {
    "async": "~1.4"
  },
  "peerDependencies": {
    "turbasen": "~1"
  },
  "devDependencies": {
    "express": "~4",
    "jshint": "~2",
    "mocha": "~2",
    "supertest": "~1"
  },
  "engines": {
    "node": ">=0.10",
    "iojs": ">=1"
  }
}
